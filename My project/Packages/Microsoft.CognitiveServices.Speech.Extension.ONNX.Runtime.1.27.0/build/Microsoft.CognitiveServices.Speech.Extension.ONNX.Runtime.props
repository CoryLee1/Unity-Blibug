<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup>
    <OnnxRuntimePackageRoot>$(MSBuildThisFileDirectory)..\</OnnxRuntimePackageRoot>
    <OnnxRuntimePath Condition="'$(Platform)' == 'ARM64'">win10-arm64</OnnxRuntimePath>
    <OnnxRuntimePath Condition="'$(Platform)' == 'ARM'">win10-arm</OnnxRuntimePath>
    <OnnxRuntimePath Condition="'$(Platform)' == 'x64'">win-x64</OnnxRuntimePath>
    <OnnxRuntimePath Condition="'$(Platform)' == 'x86' OR '$(Platform)' == 'Win32'">win-x86</OnnxRuntimePath>
    <OnnxRuntimeNativeLibDir Condition="'$(TargetPlatformIdentifier)' != 'UAP'">$(OnnxRuntimePackageRoot)build\native\$(OnnxRuntimeNativePlatform)\Release</OnnxRuntimeNativeLibDir>
    <OnnxRuntimeNativeLibDir Condition="'$(TargetPlatformIdentifier)' == 'UAP'">$(OnnxRuntimePackageRoot)build\native\uap\$(OnnxRuntimeNativePlatform)\Release</OnnxRuntimeNativeLibDir>
    <OnnxRuntimeNativeDllDir Condition="'$(TargetPlatformIdentifier)' == 'UAP'">$(OnnxRuntimePackageRoot)runtimes\$(OnnxRuntimePath)\nativeassets\uap</OnnxRuntimeNativeDllDir>
    <OnnxRuntimeNativeDllDir Condition="'$(TargetPlatformIdentifier)' != 'UAP'">$(OnnxRuntimePackageRoot)runtimes\$(OnnxRuntimePath)\native</OnnxRuntimeNativeDllDir>
  </PropertyGroup>
</Project>