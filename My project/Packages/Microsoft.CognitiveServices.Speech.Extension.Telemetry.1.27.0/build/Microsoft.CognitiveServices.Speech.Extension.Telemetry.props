<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup>
    <TelemetryPackageRoot>$(MSBuildThisFileDirectory)..\</TelemetryPackageRoot>
    <TelemetryPath Condition="'$(Platform)' == 'ARM64'">win10-arm64</TelemetryPath>
    <TelemetryPath Condition="'$(Platform)' == 'ARM'">win10-arm</TelemetryPath>
    <TelemetryPath Condition="'$(Platform)' == 'x64'">win-x64</TelemetryPath>
    <TelemetryPath Condition="'$(Platform)' == 'x86' OR '$(Platform)' == 'Win32'">win-x86</TelemetryPath>
    <TelemetryNativeLibDir Condition="'$(TargetPlatformIdentifier)' != 'UAP'">$(TelemetryPackageRoot)build\native\$(TelemetryNativePlatform)\Release</TelemetryNativeLibDir>
    <TelemetryNativeLibDir Condition="'$(TargetPlatformIdentifier)' == 'UAP'">$(TelemetryPackageRoot)build\native\uap\$(TelemetryNativePlatform)\Release</TelemetryNativeLibDir>
    <TelemetryNativeDllDir Condition="'$(TargetPlatformIdentifier)' == 'UAP'">$(TelemetryPackageRoot)runtimes\$(TelemetryPath)\nativeassets\uap</TelemetryNativeDllDir>
    <TelemetryNativeDllDir Condition="'$(TargetPlatformIdentifier)' != 'UAP'">$(TelemetryPackageRoot)runtimes\$(TelemetryPath)\native</TelemetryNativeDllDir>
  </PropertyGroup>
</Project>