<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup>
    <TelemetryNativePlatform>$(Platform)</TelemetryNativePlatform>
  </PropertyGroup>
  <Import Project="$(MSBuildThisFileDirectory)\..\Microsoft.CognitiveServices.Speech.Extension.Telemetry.props" />
    <ItemDefinitionGroup>
    <ClCompile>
    </ClCompile>
    <ResourceCompile>
    </ResourceCompile>
    <Link>
      <AdditionalDependencies>$(TelemetryNativeLibDir)\Microsoft.CognitiveServices.Speech.extension.telemetry.lib;%(AdditionalDependencies)</AdditionalDependencies>
    </Link>
  </ItemDefinitionGroup>
  <Target Name="TelemetryRedistAfterBuild" AfterTargets="AfterBuild" >
    <Copy SourceFiles="$(TelemetryNativeDllDir)\Microsoft.CognitiveServices.Speech.extension.telemetry.dll" DestinationFolder="$(TargetDir)" SkipUnchangedFiles="true" />
  </Target>
</Project>
